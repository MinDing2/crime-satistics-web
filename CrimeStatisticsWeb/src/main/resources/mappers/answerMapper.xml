<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Answer">


<!-- 댓글 작성 -->
<insert id="AnswerWrite" parameterType="com.spring.answer.vo.AnswerVo">
	insert into answer( adminid, answer_cont, create_date)
		value(#{adminid}, #{answer_cont}, #{create_date})
</insert>



</mapper>